public class Cell {
	private int score;
	private Cell prev;

	public Cell(int score) {
		prev = null;
		this.score = score;
	}

	public void setScoreAndPrev(Cell left, Cell diag, Cell under) {
		score = Math.max(Math.max(left.score, diag.score), under.score);
		if (score == left.score) {
			prev = left;
		} else if (score == diag.score) {
			prev = diag;
		} else {
			prev = under;
		}
	}

	public int getScore() {
		return score;
	}

	public Cell getPrev() {
		return prev;
	}
}
